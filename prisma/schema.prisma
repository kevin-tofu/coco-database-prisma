generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Info {
  id           Int      @id
  year         Int
  version      String
  description  String
  contributor  String
  url          String
  date_created DateTime
}

model Image {
  id            Int      @id
  width         Int
  height        Int
  file_name     String
  license       Int
  flickr_url    String
  coco_url      String
  date_captured DateTime
  coco          COCO     @relation(fields: [id], references: [id], onDelete: Cascade)
}

model Annotation {
  id           Int     @id
  image_id     Int
  category_id  Int
  segmentation Float[]
  area         Float
  bbox         Float[]
  iscrowd      Int
  coco         COCO    @relation(fields: [id], references: [id], onDelete: Cascade)
}

model Categories {
  id            Int    @id
  name          String
  supercategory String
  coco          COCO   @relation(fields: [id], references: [id], onDelete: Cascade)
}

model License {
  id   Int    @id
  name String
  url  String
  coco COCO   @relation(fields: [id], references: [id], onDelete: Cascade)
}

model COCO {
  id          Int          @id @default(autoincrement())
  image       Image[]
  annotations Annotation[]
  licenses    License[]
  categories  Categories[]
  dataset     Datasets     @relation(fields: [id], references: [id], onDelete: Cascade)
}

model Datasets {
  id   Int    @id @default(autoincrement())
  coco COCO[]
}
